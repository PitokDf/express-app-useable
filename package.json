{
  "name": "server",
  "version": "1.0.0",
  "main": "dist/src/index.js",
  "_moduleAliases": {
    "@": "src"
  },
  "scripts": {
    "dev": "tsx watch -r module-alias/register -r dotenv/config src/index.ts",
    "lint": "NODE_NO_WARNINGS=1 eslint --config eslint.config.cjs 'src/**/*.{ts,js}' --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "NODE_NO_WARNINGS=1 eslint --config eslint.config.cjs 'src/**/*.{ts,js}' --fix",
    "build": "tsc && tsc-alias",
    "start": "node -r module-alias/register dist/src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:reset": "npx prisma migrate reset --force",
    "db:prepare": "npx prisma generate"
  },
  "prisma": {
    "seed": "tsx -r module-alias/register -r dotenv/config prisma/db/seed.ts"
  },
  "keywords": [
    "service",
    "express",
    "backend",
    "docker"
  ],
  "dependencies": {
    "@prisma/client": "^6.11.1",
    "@types/cors": "^2.8.19",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.0.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.1",
    "node-cache": "^5.1.2",
    "prisma": "^6.11.1",
    "redis": "^4.7.0",
    "uuid": "^12.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.25.74"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/express-form-data": "^2.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/memory-cache": "^0.2.5",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.0.10",
    "@types/nodemailer": "^6.4.17",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-unused-imports": "^4.2.0",
    "jest": "^30.1.3",
    "module-alias": "^2.2.3",
    "prettier": "^3.6.2",
    "supertest": "^7.1.1",
    "ts-jest": "^29.4.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "overrides": {
    "glob": "^11.0.3",
    "cloudinary": "^2.7.0"
  },
  "author": "Pito Desri Pauzi",
  "license": "MIT",
  "description": "Ini adalah template untuk project express js + typecript + prisma orm"
}